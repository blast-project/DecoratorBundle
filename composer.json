{
    "name": "libre-informatique/decorator-bundle",
    "type": "symfony-bundle",
    "description": "This is the basis of a GUI for Libre Informatique Symfony2 projects",
    "require": {
        "php": ">=5.6",
        "symfony/symfony": ">=3.2",
        "sonata-project/doctrine-orm-admin-bundle":  ">=3.1",
        "blast-project/core-bundle": ">=0.4"
    },
    "require-dev": {
	"symfony/phpunit-bridge": ">=3.2"
    },
    "license": "GPL-3.0",
    "keywords": [
        "e-venement",
        "libio",
        "sonata",
        "symfony"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyBlastCoreBundle",
    "authors": [
        {
            "name": "Baptiste SIMON",
            "email": "baptiste.simon@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Librinfo\\DecoratorBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev",
            "dev-wip-lisem": "0.4.x-dev"
        }
    },
    "suggest": {
        "libre-informatique/crm-bundle": "Manage contacts, organisms and more..."
    }
}
